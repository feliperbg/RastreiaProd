{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "type": "object",
  "required": [
    "_id",
    "__v",
    "componenteConclusao",
    "departamentoResponsavel",
    "funcionariosResponsaveis",
    "nome",
    "procedimentos",
    "sequencias"
  ],
  "properties": {
    "_id": {
      "$ref": "#/$defs/ObjectId"
    },
    "__v": {
      "type": "integer"
    },
    "componenteConclusao": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/ObjectId"
      }
    },
    "departamentoResponsavel": {
      "type": "string"
    },
    "funcionariosResponsaveis": {
      "type": "array",
      "items": {
        "$ref": "#/$defs/ObjectId"
      }
    },
    "nome": {
      "type": "string"
    },
    "procedimentos": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "sequencias": {
      "type": "array",
      "items": {
        "type": "string"
      }
    }
  },
  "$defs": {
    "ObjectId": {
      "type": "object",
      "properties": {
        "$oid": {
          "type": "string",
          "pattern": "^[0-9a-fA-F]{24}$"
        }
      },
      "required": [
        "$oid"
      ],
      "additionalProperties": false
    }
  }
}